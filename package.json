{
  "name": "slush-react-express",
  "version": "4.3.0",
  "description": "A generator for a node express web app using react",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/peterjuras/slush-react-express.git"
  },
  "scripts": {
    "test": "tsc && npm run test-small && mocha -R spec test/full-builds.js --require env-test",
    "test-small": "mocha -R spec test/test.js --require env-test --skip-install",
    "clean": "rm -rf slushfile.js test/full-builds.js test/test.js test/full-builds test/util/mock.js"
  },
  "keywords": [
    "slushgenerator",
    "node",
    "react",
    "express",
    "jspm"
  ],
  "files": [
    "slushfile.js",
    "templates"
  ],
  "author": "Peter Juras",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/peterjuras/slush-react-express/issues"
  },
  "homepage": "https://github.com/peterjuras/slush-react-express#readme",
  "dependencies": {
    "empty-dir": "^0.2.0",
    "event-stream": "^3.3.1",
    "gulp": "^3.9.0",
    "gulp-conflict": "^0.4.0",
    "gulp-filter": "3.0.1",
    "gulp-install": "^0.6.0",
    "gulp-json-editor": "^2.2.1",
    "gulp-rename": "^1.2.2",
    "gulp-template": "^4.0.0",
    "inquirer": "^1.0.0",
    "run-sequence": "^1.1.4",
    "validate-npm-package-name": "^2.2.2"
  },
  "devDependencies": {
    "del": "^2.1.0",
    "env-test": "^1.0.0",
    "mkdirp": "^0.5.1",
    "mocha": "^2.3.3",
    "mock-gulp-dest": "^0.1.1",
    "should": "^11.0.0",
    "typescript": "^1.6.2"
  }
}
